0 & \nicefrac{120}{216} & \nicefrac{80}{216} & \nicefrac{15}{216} & \nicefrac{1}{216}\\
0 & 0 & \nicefrac{25}{36} & \nicefrac{10}{36} & \nicefrac{1}{36}\\
0 & 0 & 0 & \nicefrac{5}{6} & \nicefrac{1}{6}\\
0 & 0 & 0 & 0 & 1
\end{pmatrix}^{3}
\]

\end_inset


\end_layout

\begin_layout Standard
